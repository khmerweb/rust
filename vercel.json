{
    "functions": {
        "api/**/*.rs": {
            "runtime": "vercel-rust@4.0.9"
        }
    },
    "builds": [
    {
      "src": "api/**/*.rs",
      "use": "@vercel/static-build"
    }
  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/handler.rs"
        }
    ]
}